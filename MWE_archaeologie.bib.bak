@Article{Marengo_1988,
  Title                    = {L'agorà di Cirene in età romana alla luce delle testimonianze epigrafiche},
  Author                   = {Silvia Maria Marengo},
  Journal                  = {Mélanges de l'Ecole française de Rome. Antiquité},
  Pages                    = {87--101},
  Volume                   = {100},
  Year                     = {1988},
  Number                   = {1},

  Abstract                 = {Silvia Maria Marengo, L'agorà di Cirene in età romana alla luce delle testimonianze epigrafiche, p. 87-101. Viene presa in esame la documentazione epigrafica riferibile all'agorà di Cirene dal 96 a.C, inizio del protettorato di Roma sulla regione, fino a tutta l'età romana, con particolare attenzione per le testimonianze di natura politica. La rassegna comprende 35 testi che in diversa misura contribuiscono a delineare alcuni aspetti della vita della città : rapporti col governo di Roma e i suoi delegati, episodi di evergetismo, culti e tradizioni religiose, fasti delle aristocrazie locali. La ricchezza numerica e l'importanza dei documenti epigrafici inquadrabili in età augustea e primo impériale invitano a considerare corne, soprattutto in questa epoca, l'agorà abbia costituito il centro monumentale e idéale della vita pubblica, (v. retro) in accordo con i dati archeologici che testimoniano, accanto a restauri ed abbellimenti, anche interventi di rilievo politico corne l'adattamento di un preesistente edificio a sede dell'Augusteo. A partire dal II secolo le fonti iscritte che ci sono pervenute si diradano fino a tacere del tutto.},
  Comment                  = {10.3406/mefr.1988.1582},
  ISSN                     = {0223-5102},
  Language                 = {ita},
  Publisher                = {École française de Rome}
}

